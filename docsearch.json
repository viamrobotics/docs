{
    "index_name": "docsearch",
    "sitemap_urls": ["https://docs.viam.com/sitemap.xml"],
    "start_urls": ["https://docs.viam.com/"],
    "use_anchors": true,
    "selectors": {
        "lvl0": ".td-content h1",
        "lvl1": ".td-content .lead",
        "lvl2": ".td-content h2",
        "lvl3": ".td-content h3",
        "lvl4": ".td-content h4",
        "lvl5": ".td-content h5",
        "text": ".td-content p, .td-content div, .td-content ul li, .td-content ol li, .td-content table tbody tr"
    },
    "selectors_exclude": [".footer", "#edit-on-github"]
}
